pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
sprites={}

empty=0
baba=1
wall=2
rock=3
flag=4
lava=5

name=0+16 -- offset from tile to name
name1st=16
namelst=31

rule1st=32
is=32
you=33
stp=34
push=35
win=36
kill=37
rulelst=47

is_win=false
jitter=1.1
normal_jitter=1.1
dirty=true

rules={}

function is_op(m)
 return m==is
end

function is_name(m)
 return m>=name1st and m<=namelst
end

function is_rule(m)
 return m>=rule1st and m<=rulelst
end

function make_rules()
 rules={}
 for y=0,127 do
  for x=0,127 do
   local m=mget(x,y)
   if is_op(m) then
    local a=mget(x-1,y)
    local b=mget(x+1,y)
    if is_name(a) and is_rule(b) then
     add(rules,{a-name,m,b})
    end
    a=mget(x,y-1)
    b=mget(x,y+1)
    if is_name(a) and is_rule(b) then
     add(rules,{a-name,m,b})
    end
   end
  end
 end
 for z=name1st,rulelst do
  add(rules,{z,is,push})
 end
end

function test()
 rules={
  {baba, is, you},
  {wall, is, stp},
  {lava, is, kill},
  {rock, is, push},
  {flag, is, win},
 }
 assert(rule(baba,is,you))
 assert(not rule(rock,is,you))
end

function make_sprite(x,y,t)
 return {
  x=x,
  y=y,
  t=t,
  update=update_sprite,
  draw=draw_sprite
 }
end

function _mset(x,y,t)
 mset(x,y,t)
 dirty=true
end

function lift(thing)
 for y=0,127 do
  for x=0,127 do
   if mget(x,y)==thing then
    _mset(x,y,empty)
    s=make_sprite(x,y,thing)
    add(sprites,s)
   end
  end
 end
end

function rule(a,op,b)
 for r in all(rules) do
  if a==r[1] and op==r[2] and b==r[3] then
   return true
  end
 end
 return false
end

function try_move(s,x,y)
 if x<0 or x>15 or y<0 or y>15 then
  return false
 end
 if s.x==x and s.y==y then return end
 local m=mget(x,y)
 if rule(m,is,push) then
  if not try_push(empty,x,y,x-s.x,y-s.y) then
   return
  end
 end
 for r in all(rules) do
  local a=r[1]
  local op=r[2]
  local b=r[3]
  if op==is and a==m then
   if b==stp then
    return
   end
   if b==kill then
    del(sprites,s)
   end
   if b==win and rule(s.t,is,you) then
    is_win=true
   end
  end
 end
 s.x=x
 s.y=y
end

function try_push(m,x,y,dx,dy)
 if x<0 or x>15 or y<0 or y>15 then
  return false
 end
 
 local z=mget(x,y)
 if rule(z,is,stp) then
  return false
 end

 if z==empty then
  _mset(x,y,m)
  return true
 end
 if rule(z,is,push) and try_push(z,x+dx,y+dy,dx,dy) then
  _mset(x,y,m)
  return true
 end
 
 return false
end

function update_sprite(s)
 if rule(s.t,is,you) then
  local x=s.x
  local y=s.y
  if btnp(0) then x-=1 end
  if btnp(1) then x+=1 end
  if btnp(2) then y-=1 end
  if btnp(3) then y+=1 end
  try_move(s,x,y)
 end
end

function draw_sprite(s)
 spr(s.t,s.x*8+rnd(jitter),s.y*8+rnd(jitter))
end

function _init()
 test()
 make_rules()
 lift(baba)
end

function _update()
 for s in all(sprites) do
  s:update()
 end
 if dirty then
  make_rules()
 end
 dirty=false
end

function _draw()
 cls()
 map(0,0,0,0)
 for s in all(sprites) do
  s:draw()
 end
 if is_win then
  print("win!!!",52+rnd(2),60+rnd(2),rnd(16))
 end
 jitter=jitter*0.9+normal_jitter*0.1
end

