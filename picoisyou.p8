pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
-- pico is you
-- a demake of baba is you
-- original by @hempuli
-- demake by @jminor

empty=0
baba=1
wall=2
rock=3
flag=4
lava=5
border=6

name=0+16 -- offset from tile to name
name1st=16
namelst=31

rule1st=32
is=32
you=33
stp=34
push=35
win=36
kill=37
rulelst=47

is_win=false

normal_jitter=1.001
bump_jitter=5
kill_jitter=20
win_jitter=20

dirty=true

sprites={}
moved={}

rules={}

function is_op(m)
 return m==is
end

function is_name(m)
 return m>=name1st and m<=namelst
end

function is_rule(m)
 return m>=rule1st and m<=rulelst
end

function replace(a,b)
 for s in all(sprites) do
  if s.t==a then s.t=b end
 end
end

function at(x,y)
 local result={}
 for s in all(sprites) do
  if s.x==x and s.y==y then
   add(result,s)
  end
 end
 return result
end

function make_rule(a,op,b)
 if is_name(a) then
  if is_rule(b) then
   add(rules,{a-name,op,b})
  elseif is_name(b) then
   replace(a-name,b-name)
  end
 end
end

function make_rules()
 local a
 local b
 rules={}
 -- always do these
 for z=name1st,rulelst do
  add(rules,{z,is,push})
 end
 add(rules,{border,is,stp})
 -- do in order for robustness
 -- but too slow...
 --for y=0,127 do
 -- for x=0,127 do
 --  for s in all(at(x,y)) do
 for s in all(sprites) do
  local x=s.x
  local y=s.y
    if is_op(s.t) then
     a=at(x-1,y)
     b=at(x+1,y)
     if #a>0 and #b>0 then
      make_rule(a[1].t,s.t,b[1].t)
     end
     a=at(x,y-1)
     b=at(x,y+1)
     if #a>0 and #b>0 then
      make_rule(a[1].t,s.t,b[1].t)
     end
    end
   end
--  end
-- end
end

function test()
 rules={
  {baba, is, you},
  {wall, is, stp},
  {lava, is, kill},
  {rock, is, push},
  {flag, is, win},
 }
 assert(rule(baba,is,you))
 assert(not rule(rock,is,you))
 rules={}
end

function make_sprite(x,y,t)
 return {
  x=x,
  y=y,
  px=rnd(128),
  py=rnd(128),
  t=t,
  update=update_sprite,
  draw=draw_sprite
 }
end

function read_map()
 sprites={}
 for y=0,127 do
  for x=0,127 do
   local thing=mget(x,y)
   if thing!=0 then
    s=make_sprite(x,y,thing)
    add(sprites,s)
   end
  end
 end
end

function rule(a,op,b)
 for r in all(rules) do
  if a==r[1] and op==r[2] and b==r[3] then
   return true
  end
 end
 return false
end

function try_move(s,dx,dy)
 if dx==0 and dy==0 then
  return false
 end

 local x=s.x+dx
 local y=s.y+dy
 if x<0 or x>15 or y<0 or y>15 then
  jitter(s,bump_jitter)
  return false
 end

 -- what's in our way?
 for o in all(at(x,y)) do
 
  if rule(o.t,is,stp) then
   jitter(o,bump_jitter)
   return false
  end
  
  if rule(o.t,is,push) then
   if not try_move(o,dx,dy) then
    jitter(o,bump_jitter)
    return false
   end
  end
  
  if rule(o.t,is,kill) then
   del(sprites,s)
   jitter(o,kill_jitter)
  end
  
  if rule(o.t,is,win) and rule(s.t,is,you) then
   is_win=true
   jitterall(win_jitter)
  end
 end
 
 -- okay, we can move
 s.x=x
 s.y=y
 add(moved,s)
 dirty=true
 
 return true
end

function update_sprite(s)
 if rule(s.t,is,you) then
  local dx=0
  local dy=0
  if btnp(0) then dx-=1 end
  if btnp(1) then dx+=1 end
  if btnp(2) then dy-=1 end
  if btnp(3) then dy+=1 end
  if dx!=0 or dy!=0 then
   if not try_move(s,dx,dy) then
    jitter(s,bump_jitter)
   end
  end
 end
end

function jitter(s,j)
 s.px+=rnd(j)-j/2
 s.py+=rnd(j)-j/2
end

function jitterall(j)
 for s in all(sprites) do
  jitter(s,j)
 end
end

function draw_sprite(s)
 local t=0.5
 local tm1=1-t
 local j=rnd(normal_jitter)
 s.px=s.px*t + (s.x*8+j)*tm1
 s.py=s.py*t + (s.y*8+j)*tm1
 spr(s.t,s.px,s.py)
end

function _init()
 test()
 read_map()
 make_rules()
-- for r in all(rules) do
--  print("rule:"..r[1].." "..r[2].." "..r[3])
-- end
end

function _update()
 moved={}
 for s in all(sprites) do
  s:update()
 end
 if dirty then
  make_rules()
  dirty=false
 end
 -- any sprites that moved,
 -- we pop to the top
 for s in all(moved) do
  del(sprites,s)
  add(sprites,s)
 end
end

function _draw()
 cls()
 --map(0,0,0,0)
 for s in all(sprites) do
  s:draw()
 end
 if is_win then
  print("win!!!",52+rnd(2),60+rnd(2),rnd(16))
 end
end

